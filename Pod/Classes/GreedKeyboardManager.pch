#ifndef WeakSelf
#define WeakSelf(weakSelf)  __weak __typeof(&*self)weakSelf = self;
#endif

#ifndef StrongSelf
#define StrongSelf(strongSelf)  __strong __typeof(&*self)strongSelf = weakSelf;
#endif
